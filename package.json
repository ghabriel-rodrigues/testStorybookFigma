{
  "name": "@gupy/design-system-v2",
  "version": "5.0.0",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "browser": "./dist/index.umd.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "prepare": "husky install",
    "test": "npm run test:ts && vitest run --passWithNoTests",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:ts": "tsc --noEmit",
    "coverage": "vitest run --coverage",
    "lint": "eslint --ext \".js,.ts,.tsx\" --ignore-path .gitignore .",
    "lint:fix": "eslint --ext \".js,.ts,.tsx\" --ignore-path .gitignore . --fix",
    "tokens": "style-dictionary build -c ./src/_services/tokens/config.json",
    "generate": "plop --plopfile ./src/_services/generators/component.cjs",
    "generate:icon": "plop --plopfile ./src/_services/generators/icon.cjs",
    "generate:dts": "tsc --skipLibCheck --declaration --emitDeclarationOnly --outDir dist",
    "storybook": "start-storybook -p 6006 -c ./src/_services/documentation",
    "build:storybook": "build-storybook -c ./src/_services/documentation",
    "build:rollup": "npm run tokens && npm run generate:dts && cross-env NODE_ENV=production rollup --config ./src/_services/build/rollup.config.js",
    "release": "lerna publish ${npm_config_version_keyword} --registry=${npm_config_registry} --force-publish=* --conventional-commits -y",
    "pre-release": "lerna publish --canary --preid ${npm_config_preid} ${npm_config_version_keyword} --registry=${npm_config_registry} --force-publish=* --conventional-commits -y"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.5"
  },
  "dependencies": {
    "@accessible/button": "^2.0.2",
    "@accessible/use-conditional-focus": "^3.0.1",
    "@accessible/use-key": "^1.0.2",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@radix-ui/react-collapsible": "^1.0.0",
    "@radix-ui/react-dialog": "^0.1.7",
    "@radix-ui/react-polymorphic": "^0.0.14",
    "@radix-ui/react-slot": "^0.1.2",
    "@radix-ui/react-toggle": "^0.1.4",
    "@radix-ui/react-toggle-group": "^0.1.5",
    "@react-aria/button": "^3.4.4",
    "@react-aria/radio": "^3.3.0",
    "@react-aria/searchfield": "^3.2.6",
    "@react-aria/switch": "^3.1.8",
    "@react-aria/tooltip": "^3.1.8",
    "@react-aria/utils": "^3.12.0",
    "@react-aria/visually-hidden": "^3.2.8",
    "@react-hook/merged-ref": "^1.3.2",
    "@react-stately/radio": "^3.5.0",
    "@react-stately/searchfield": "^3.1.6",
    "@react-stately/toggle": "^3.2.7",
    "@react-stately/tooltip": "^3.0.9",
    "nanoid": "^3.3.2",
    "react-input-mask": "^2.0.4",
    "react-select": "^5.2.2",
    "styled-system": "^5.1.5",
    "ts-dedent": "^2.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@react-aria/button": "^3.4.4",
    "@react-aria/combobox": "^3.2.6",
    "@react-hook/previous": "^1.0.1",
    "@react-stately/combobox": "^3.0.5",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.0",
    "@storybook/addon-a11y": "^6.5.9",
    "@storybook/addon-actions": "^6.4.20",
    "@storybook/addon-docs": "^6.4.20",
    "@storybook/addon-essentials": "^6.4.20",
    "@storybook/addon-links": "^6.4.20",
    "@storybook/builder-vite": "^0.1.24",
    "@storybook/builder-webpack5": "^6.4.20",
    "@storybook/manager-webpack5": "^6.4.20",
    "@storybook/node-logger": "^6.4.20",
    "@storybook/preset-create-react-app": "^4.1.0",
    "@storybook/react": "^6.4.20",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest-axe": "^3.5.4",
    "@types/react": "17.0.39",
    "@types/react-dom": "^17.0.10",
    "@types/react-input-mask": "^3.0.1",
    "@types/styled-components": "^5.1.25",
    "@types/styled-system": "^5.1.15",
    "@types/styled-system__css": "^5.0.16",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "@vitejs/plugin-react": "^1.0.7",
    "@vitest/ui": "^0.6.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.8.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-design-system-v2": "file:src/_services/rules",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-styled-components-a11y": "0.0.34",
    "husky": "^7.0.4",
    "jest-axe": "^6.0.0",
    "jsdom": "^19.0.0",
    "lerna": "^4.0.0",
    "lerna-changelog": "^2.2.0",
    "lint-staged": "^12.3.7",
    "plop": "2.7.4",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "style-dictionary": "^3.1.1",
    "styled-components": "^5.3.5",
    "ts-node": "^10.5.0",
    "typescript": "^4.4.4",
    "vite": "^2.8.4",
    "vitest": "^0.6.0"
  },
  "engines": {
    "node": ">=14.18.3",
    "npm": ">=6.14.15"
  }
}
